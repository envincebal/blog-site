<%- include("partials/header") -%>
  <div class="signup-container">
    <h1>Sign Up</h1>
    <form action="/sign-up" method="POST">
      <div class="form-group">
        <label for="username">Username</label>
        <input name="username" placeholder="username" type="text" class="form-control" id="postTitle" autocomplete="off" />
        <label for="password">Password</label>
        <input name="password" type="password" class="form-control" id="postTitle" autocomplete="off" />
      </div>
      <button type="submit" name="button" class="btn btn-primary">Log In</button>
      <p>Already have an account? Log in <a href="/">HERE</a></p>
    </form>
  </div>

  <%- include("partials/footer") -%>